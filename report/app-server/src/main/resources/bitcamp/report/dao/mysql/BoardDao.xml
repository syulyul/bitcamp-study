<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bitcamp.report.dao.BoardDao">
  
  <insert id = "insert" parameterType="bitcamp.report.vo.Board">
    insert into report_board(title,content,writer,category)
    values(#{title},#{content},#{writer.no},#{category})
  </insert>
</mapper>